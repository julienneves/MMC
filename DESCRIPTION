Package: mmc
Type: Package
Title: Maximized Monte Carlo
Version: 0.1.0.9012
Authors@R: c(person("Julien", "Neves", email = "neves.julien@gmail.com", role = c("aut", "cre")), person("Jean-Marie", "Dufour", email = "jean-marie.dufour@mcgill.ca", role = c("aut", "ths")))

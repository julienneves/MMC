  \entry{david_a._dickey_determining_1987}{article}{}
    \name{author}{1}{}{%
      {{hash=DSGP}{%
         family={{David A. Dickey}},
         familyi={D\bibinitperiod},
         given={Sastry G\bibnamedelima Pantula},
